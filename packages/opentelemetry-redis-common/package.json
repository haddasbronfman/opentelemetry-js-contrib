{
  "name": "@opentelemetry/redis-common",
  "version": "0.33.0",
  "description": "Redis utilities for redis instrumentations",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
